     }
	},
	"scripts": {
     	"post-update-cmd": [
           "Fbourigault\\ComposerMkdir\\ScriptHandler::mkdirs",
           "php -r \"copy('grumphp.custom.yml', 'grumphp.yml');\"",
           "php -r \"copy('./vendor/lavary/crunz/crunz.yml', './crunz.yml');\"",
           "php -r \"exec('./vendor/bin/robo create_upload_folder');\"",
           "php -r \"chmod('./public/uploads/', 0777);\""
       ]
     },
